<Project>

	<Import Project="version.props" />

	<PropertyGroup Label="Build">
		<TargetFrameworks>net461;netstandard2.1;netcoreapp3.1</TargetFrameworks>
		<NoWarn>$(NoWarn);1591;1587</NoWarn>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<PropertyGroup Label="Package">
		<Product>MR.AspNetCore.MvcPack</Product>
		<Authors>Mohammad Rahhal @mrahhal</Authors>
		<PackageProjectUrl>https://github.com/mrahhal/MR.AspNetCore.MvcPack</PackageProjectUrl>
		<PackageLicenseUrl>https://github.com/mrahhal/MR.AspNetCore.MvcPack/blob/master/LICENSE.txt</PackageLicenseUrl>
	</PropertyGroup>

</Project>
